{
  "tests": [
    {
      "name": "health_check",
      "input": {
        "method": "health"
      },
      "expectedOutput": {
        "status": "success"
      },
      "timeout": 10000
    },
    {
      "name": "predict_with_url",
      "input": {
        "method": "predict",
        "image_url": "https://ultralytics.com/images/bus.jpg"
      },
      "expectedOutput": {
        "status": "success",
        "data": {
          "model": "yolov8n.pt"
        }
      },
      "timeout": 30000
    },
    {
      "name": "predict_with_base64",
      "input": {
        "method": "predict",
        "image_base64": ""
      },
      "expectedOutput": {
        "status": "success",
        "data": {
          "model": "yolov8n.pt"
        }
      },
      "timeout": 30000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA GeForce RTX 4090",
    "gpuCount": 1,
    "env": [
      {
        "key": "MODEL_NAME",
        "value": "yolov8n.pt"
      },
      {
        "key": "DEVICE",
        "value": "cuda"
      },
      {
        "key": "CONF_THRESHOLD",
        "value": "0.25"
      },
      {
        "key": "IOU_THRESHOLD",
        "value": "0.45"
      }
    ],
    "allowedCudaVersions": [
      "12.4",
      "12.3",
      "12.2",
      "12.1",
      "12.0",
      "11.8"
    ]
  }
}
